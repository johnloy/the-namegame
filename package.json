{
  "name": "the-namegame",
  "description": "A fun way to associate names with faces, using a spaced repetition algorithm",
  "author": "John Loy <john@johnloy.net> (http://github.com/johnloy)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnloy/the-name-game"
  },
  "homepage": "https://github.com/johnloy/the-name-game",
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "clean": "rm ./dist/*",
    "build": "npm run build:assets && npm run build:html",
    "build:assets": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "build:html": "node ./bin/fix-webpack-stats.js && node ./bin/build-html.js > ./dist/200.html",
    "deploy": "npm run build && surge -p dist -d wat-namegame.surge.sh",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "standard"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [],
    "global": [
      "__DEVELOPMENT__",
      "__SERVER__",
      "__CLIENT__",
      "__DISABLE_SSR__",
      "webpackIsomorphicTools"
    ]
  },
